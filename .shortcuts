runPhenotips(){
    cd /Users/Dean/phenotips-allfiles/myFork/phenotips/distribution/standalone/target/
    if [[ -d phenotips-standalone-1.2-SNAPSHOT ]]
    then
        ./phenotips-standalone-1.2-SNAPSHOT/start.sh 
    elif [[ -e phenotips-standalone-1.2-SNAPSHOT.zip ]]
    then
        unzip phenotips-standalone-1.2-SNAPSHOT.zip  
